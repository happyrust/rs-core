--从节点出发找到根节点，默认层级深度为 9
let $a = SELECT id, ->pe_owner->(? AS f1)->pe_owner->(? AS f2)->pe_owner->(? AS f3)->pe_owner->(? AS f4)->pe_owner->(? AS f5)->pe_owner->(? AS f6)->pe_owner->(? AS f7)->pe_owner->(? AS f8)->pe_owner->(? AS f9)  from only (type::thing("pe", $refno));
array::flatten([$a['id'].refno.*, $a['f1'].refno.*, $a['f2'].refno.*, $a['f3'].refno.*, $a['f4'].refno.*, $a['f5'].refno.*, $a['f6'].refno.*, $a['f7'].refno.*, $a['f8'].refno.*, $a['f9'].refno.*]);